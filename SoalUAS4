package uasfix2022;

public class SoalUAS4 {
	public static void main(String[] args) {
		int n = 5;
		int x,y;
		for(x=1; x<=n; x++)
		{
			for(y=1; y<=n-x+1; y++)
			{
				System.out.print(" ");
			} 
			int num = 0;
			for(y=1; y<=x; y++)
			{
				System.out.print(num);
				num++;
			} 
			System.out.println();
		}
	}
}
